##########################################
# Playerbot Configuration file           #
##########################################

# 启用或禁用 AI Playerbot
AiPlayerbot.Enabled = 1

# 启用随机机器人系统
AiPlayerbot.RandomBotAutologin = 1

# 开始时登录所有随机机器人
AiPlayerbot.RandomBotLoginAtStartup = 1

# 删除所有随机机器人帐户（重置随机机器人）
AiPlayerbot.DeleteRandomBotAccounts = 0

# 在玩家登录时自动登录所有玩家 alts 作为机器人
AiPlayerbot.BotAutologin = 0

# 公会任务系统
AiPlayerbot.EnableGuildTasks = 1

# 为随机机器人启用 LFG
AiPlayerbot.RandomBotJoinLfg = 1

# 为随机机器人启用 BG/Arena
AiPlayerbot.RandomBotJoinBG = 1

# 将许多任务 <= 机器人级别标记为已完成（减慢机器人创建速度）
AiPlayerbot.PreQuests = 1

# 随机机器人计数
AiPlayerbot.MinRandomBots = 50
AiPlayerbot.MaxRandomBots = 200
AiPlayerbot.RandomBotMinLevel = 1
AiPlayerbot.RandomBotMaxLevel = 80

# 为随机机器人创建的帐户
AiPlayerbot.RandomBotAccountPrefix = "rndbot"
AiPlayerbot.RandomBotAccountCount = 200

# 随机机器人公会计数
AiPlayerbot.RandomBotGuildCount = 20

# 删除所有随机机器人公会
AiPlayerbot.DeleteRandomBotGuilds = 0

# 随机机器人竞技场队伍数量
AiPlayerbot.RandomBotArenaTeamCount = 20

# 删除所有随机机器人竞技场团队
AiPlayerbot.DeleteRandomBotArenaTeams = 0

# 传送随机机器人的地图
AiPlayerbot.RandomBotMaps = "0,1,530,571

# 更改随机机器人的档位较低
AiPlayerbot.RandomGearLoweringChance = 0.15

# 机会随机机器人在第一次随机化时具有最大级别（默认 0.15）
AiPlayerbot.RandomBotMaxLevelChance = 0.15

# Chance bot 选择 RPG（根据他们的等级传送到随机营地）而不是研磨
AiPlayerbot.RandomBotRpgChance = 0.20

# 将随机机器人的移动速度设置为在任何地方行走
AiPlayerbot.RandombotsWalkingRPG = 0

# 将随机机器人的移动速度设置为仅在建筑物内行走
AiPlayerbot.RandombotsWalkingRPG.InDoors = 0

# 机器人向玩家打招呼
AiPlayerbot.EnableGreet = 1

# 在随机机器人上显示头盔和斗篷（需要重置）
AiPlayerbot.RandomBotShowHelmet = 1
AiPlayerbot.RandomBotShowCloak = 1

# 禁用随机机器人的随机级别
# 每个机器人都从指定的级别开始，并通过杀死暴徒来升级。
AiPlayerbot.DisableRandomLevels = 0

# 如果启用“AiPlayerbot.DisableRandomLevels”，请在此处设置随机机器人的起始级别
# Recommended: 5+
AiPlayerbot.randombotStartingLevel = 5

# 为机器人设置杀死 XP 率（默认值：1）
# Server XP Rate * AiPlayerbot.KillXPRate
AiPlayerbot.KillXPRate = 1

# 指定活跃机器人的百分比
# 默认值为 10。每分钟有 10% 的机器人处于活动状态或非活动状态。
AiPlayerbot.BotActiveAlone = 10

# 设置获得物品附魔的随机机器人的最低级别（Maxlevel + 1 禁用）
# Default: 60
AiPlayerbot.MinEnchantingBotLevel = 60

# 随机机器人检查玩家的装备分数水平，如果分数太低则拒绝组队邀请
# Default: 1 (enabled)
AiPlayerbot.GearScoreCheck = 0

# 将完成并奖励所有随机机器人的任务
AiPlayerbot.RandomBotQuestIds = "7848,3802,5505,6502,7761"

# 随机机器人将与附近的机器人分组进行共享任务
AiPlayerbot.RandomBotGroupNearby = 1

# 没有主人的机器人会说他们的台词
AiPlayerbot.RandomBotSayWithoutMaster = 0

# 自动化

# 机器人选择他们的任务奖励（yes = 选择第一个有用的项目，no = 列出所有奖励，ask = 选择有用的项目并列出多个）
AiPlayerbot.AutoPickReward = yes

# 机器人装备升级（机器人将装备从抢劫或任务中获得的任何物品，如果它们是升级的话）
# Default: 0 (disabled)
AiPlayerbot.AutoEquipUpgradeLoot = 1

# 与玩家同步任务（机器人将在您提交任务的那一刻完成任务。机器人将忽略掠夺任务物品。）
# Default: 0 (disabled)
AiPlayerbot.SyncQuestWithPlayer = 0

# 机器人会在玩家上交时自动完成任务
# Default: 0 (disabled)
AiPlayerbot.SyncQuestForPlayer = 0

# 将最大随机机器人等级与最大在线玩家等级同步
# Default: 0 (disabled)
AiPlayerbot.SyncLevelWithPlayers = 0

# 机器人在与训练师交谈时会自动训练法术（yes = 只要机器人有钱，就可以训练所有可用的法术，free  = 无需金钱成本的自动训练，no = 仅列出法术）
AiPlayerbot.AutoTrainSpells = yes

# 机器人根据当前规格自动选择天赋点（full = 如果多个规格与当前规格相似，则根据概率选择规格，semi = 仅在 1 个规格看起来与当前规格相似时应用点数，no = 没有自动天赋点数）
AiPlayerbot.AutoPickTalents = full

# 机器人在升级时自动学习可训练的法术
# Default: 0 (disabled)
AiPlayerbot.AutoLearnTrainerSpells = 0

# 机器人在升级时自动学习职业任务奖励法术
# Default: 0 (disabled)
AiPlayerbot.AutoLearnQuestSpells = 0

# 随机机器人会自行选择任务并尝试完成
# Default: 0 (disabled)
AiPlayerbot.AutoDoQuests = 0

##################################################################################
#                                                                                #
# 			所有其他参数都是可选的，但可以通过在此处取消注释来更改 				 #
#                                                                                #
##################################################################################

