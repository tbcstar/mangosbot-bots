##################################################################################
#                                                                                #
# Database Stuff 																 #
#                                                                                #
##################################################################################

#
#    PlayerbotDatabaseInfo
#        Description: playerbots 服务器的数据库连接设置。
#        Example:     "hostname;port;username;password;database"
#                     ".;somenumber;username;password;database" - (Use named pipes on Windows
#                                                                 "enable-named-pipe" to [mysqld]
#                                                                 section my.ini)
#                     ".;/path/to/unix_socket;username;password;database" - (use Unix sockets on
#                                                                           Unix/Linux)
#        Default:     "127.0.0.1;3306;acore;acore;acore_playerbots"       - (PlayerbotDatabaseInfo)

PlayerbotDatabaseInfo = "127.0.0.1;3306;acore;acore;acore_playerbots"

#
#    PlayerbotDatabase.WorkerThreads
#        Description: 为处理异步（延迟）MySQL 语句而产生的工作线程数量。
#                     每个工作线程都通过自己与 MySQL 服务器的连接
#                     以及它们在 MySQL 服务器上的自己的线程进行镜像。
#        Default:     1 - (PlayerbotDatabase.WorkerThreads)

PlayerbotDatabase.WorkerThreads     = 1

#
#    PlayerbotDatabase.SynchThreads
#        Description: 产生要处理的 MySQL 连接数量。
#        Default:     1 - (PlayerbotDatabase.WorkerThreads)

PlayerbotDatabase.SynchThreads     = 1

#    Playerbot.Updates.EnableDatabases
#        Description: 确定更新系统是否适用于 playerbot 数据库。
#
#        Default:     1  - (Enabled)
#                     0  - (Disabled)

Playerbot.Updates.EnableDatabases = 1
